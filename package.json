{
  "name": "verb-repo-data",
  "description": "Verb plugin that repository and package.json data to the context for rendering templates.",
  "version": "0.1.3",
  "homepage": "https://github.com/verbose/verb-repo-data",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-repo-data",
  "bugs": {
    "url": "https://github.com/verbose/verb-repo-data/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "camel-case": "^1.2.2",
    "expand-pkg": "^0.1.1",
    "isobject": "^2.0.0",
    "lazy-cache": "^1.0.3",
    "namify": "^0.1.3",
    "repo-utils": "^0.3.0"
  },
  "devDependencies": {
    "delete": "^0.3.0",
    "engine-base": "^0.1.2",
    "gitty": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.7",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "mocha": "^2.4.5",
    "verb": "github:verbose/verb#dev"
  },
  "keywords": [
    "data",
    "verb"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "generate",
        "verb",
        "verb-readme-generator"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
