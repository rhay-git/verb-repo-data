{
  "name": "verb-repo-data",
  "description": "Verb plugin that adds repository and package.json data to the context for rendering templates.",
  "version": "0.1.15",
  "homepage": "https://github.com/verbose/verb-repo-data",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-repo-data",
  "bugs": {
    "url": "https://github.com/verbose/verb-repo-data/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-data": "^0.6.0",
    "camel-case": "^3.0.0",
    "clone-deep": "^0.2.4",
    "expand-pkg": "^0.1.8",
    "fs-exists-sync": "^0.1.0",
    "generate-data": "^0.1.7",
    "is-valid-app": "^0.2.1",
    "lazy-cache": "^2.0.2",
    "mixin-deep": "^1.1.3",
    "namify": "^0.1.3",
    "repo-utils": "^0.3.7"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "engine-base": "^0.1.2",
    "gitty": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.11",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-unused": "^0.2.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "verb": "https://github.com/verbose/verb#dev"
  },
  "keywords": [
    "context",
    "data",
    "docs",
    "documentation",
    "generator",
    "plugin",
    "repo",
    "verb",
    "verbplugin"
  ],
  "verb": {
    "toc": "collapsible",
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ],
    "related": {
      "list": [
        "assemble",
        "generate",
        "verb",
        "verb-readme-generator"
      ]
    },
    "run": true,
    "layout": "default",
    "lint": {
      "reflinks": true
    },
    "tasks": [
      "readme"
    ]
  }
}
