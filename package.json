{
  "name": "verb-repo-data",
  "description": "Verb plugin that adds repository and package.json data to the context for rendering templates.",
  "version": "0.1.12",
  "homepage": "https://github.com/verbose/verb-repo-data",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-repo-data",
  "bugs": {
    "url": "https://github.com/verbose/verb-repo-data/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-data": "^0.5.0",
    "camel-case": "^1.2.2",
    "expand-pkg": "^0.1.3",
    "fs-exists-sync": "^0.1.0",
    "generate-data": "^0.1.3",
    "is-valid-app": "^0.1.1",
    "lazy-cache": "^2.0.1",
    "mixin-deep": "^1.1.3",
    "namify": "^0.1.3",
    "repo-utils": "^0.3.4"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "engine-base": "^0.1.2",
    "gitty": "^3.3.5",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "mocha": "^2.5.3",
    "verb": "https://github.com/verbose/verb#dev"
  },
  "keywords": [
    "data",
    "docs",
    "documentation",
    "plugin",
    "repo",
    "verb",
    "verbplugin"
  ],
  "verb": {
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ],
    "related": {
      "list": [
        "assemble",
        "generate",
        "verb",
        "verb-readme-generator"
      ]
    },
    "run": true,
    "toc": false,
    "layout": "default",
    "lint": {
      "reflinks": true
    },
    "tasks": [
      "readme"
    ]
  }
}