{
  "name": "verb-context",
  "description": "Verb plugin that adds commonly needed data to the context for rendering templates.",
  "version": "0.1.1",
  "homepage": "https://github.com/jonschlinkert/verb-context",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/verb-context",
  "bugs": {
    "url": "https://github.com/jonschlinkert/verb-context/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "git-user-name": "^1.1.2",
    "isobject": "^2.0.0",
    "lazy-cache": "^1.0.3",
    "mixin-deep": "^1.1.3",
    "omit-empty": "^0.3.3",
    "parse-author": "^0.2.1",
    "parse-github-url": "^0.3.0"
  },
  "devDependencies": {
    "delete": "^0.3.0",
    "engine-base": "^0.1.2",
    "gitty": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.7",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "mocha": "*",
    "verb": "github:verbose/verb#dev"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "generate",
        "verb",
        "verb-readme-generator"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
